{
    "name": "AutoWinClaim",
    "description": "Automatically claims the win of a 4 player chess.com game",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://www.chess.com/variants/4-player-chess/*"],
          "js": ["main-script.js"]
        }
      ],
    "host_permissions": ["https://www.chess.com/variants/4-player-chess/*"],
    "icons": {
        "16": "resources/4pc_16.png",
        "32": "resources/4pc_32.png",
        "48": "resources/4pc_48.png",
        "128": "resources/4pc_128.png"
    }
}